{
  "name": "cdk_alb_lambda",
  "version": "0.1.0",
  "bin": {
    "cdk_alb_lambda": "bin/cdk_alb_lambda.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --forceExit --detectOpenHandles",
    "test:individual": "jest --testTimeout=10000 --forceExit",
    "test:fast": "JEST_HIDE_LOGS=true jest --forceExit --detectOpenHandles --no-cache",
    "test:ci": "JEST_HIDE_LOGS=true jest --forceExit --detectOpenHandles --ci --runInBand",
    "test:lambda": "jest 'test/lambda/.*\\.test\\.ts' --forceExit",
    "test:infra": "jest 'test/constructs/.*\\.test\\.ts' --forceExit",
    "test:stack": "jest 'test/cdk_alb_lambda-stack\\.test\\.ts' --forceExit",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "aws-cdk": "2.1001.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.203.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-sqs": "^3.758.0",
    "@types/aws-lambda": "^8.10.147",
    "aws-cdk-lib": "^2.180.0",
    "aws-sdk": "^2.1692.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.25.0"
  }
}
